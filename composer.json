{
    "name": "wishtreehkumar/azureadsso",
    "description": "Azure Active Directory SSO Login flow",
    "keywords": [
        "Azure Active Directory",
        "Azure Active Directory B2C",
        "SSO",
        "Laravel"
    ],
    "homepage": "https://github.com/wishtree-hkumar/azureadsso",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Wishtreehkumar\\Azureadsso\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Hitesh",
            "email": "hitesh.kumar@wishtreetech.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.3|^8.0|^8.1",
        "illuminate/http": "^7.0|^8.0|^9.0",
        "illuminate/support": "^7.0|^8.0|^9.0",
        "phpseclib/phpseclib": "2.0.31"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wishtreehkumar\\Azureadsso\\ServiceProvider"
            ],
            "aliases": {
                "AzureADB2CToken": "Wishtreehkumar\\Azureadsso\\Facade\\AzureADB2CToken"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
